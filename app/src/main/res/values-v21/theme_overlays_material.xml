<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Base.ThemeOverlay.V21.ActivityLauncher.Material" parent="Base.ThemeOverlay.ActivityLauncher.Holo">
        <item name="listIconLarge">40dp</item>
        <item name="listPreferredItemHeightTwoLines">?android:listPreferredItemHeightLarge</item>
        <item name="textAppearanceListItem">?android:textAppearanceListItem</item>
        <item name="textAppearanceListItemSecondary">?android:textAppearanceListItemSecondary</item>
        <item name="menuSearchIcon">@drawable/ic_menu_search_material</item>
        <item name="menuRefreshIcon">@drawable/ic_menu_refresh_material</item>
        <item name="menuSortIcon">@drawable/ic_menu_sort_material</item>
        <item name="menuAboutIcon">@drawable/ic_menu_about_material</item>
        <item name="menuLaunchUriIcon">@drawable/ic_menu_launch_uri_material</item>
        <item name="dialogPreferredPadding">16dp</item>
        <item name="gridViewStyle">?android:gridViewStyle</item>
    </style>

    <style name="Base.ThemeOverlay.ActivityLauncher.Material" parent="Base.ThemeOverlay.V21.ActivityLauncher.Material" />

    <style name="ThemeOverlay.ActivityLauncher.Material.Light" parent="Base.ThemeOverlay.ActivityLauncher.Material">
        <item name="android:colorAccent">@color/color_accent_material</item>
    </style>

    <style name="ThemeOverlay.ActivityLauncher.Material.Light.DarkActionBar" parent="Base.ThemeOverlay.ActivityLauncher.Material">
        <item name="android:colorPrimary">@color/color_primary_material</item>
        <item name="android:colorPrimaryDark">@color/color_primary_dark_material</item>
        <item name="android:colorAccent">@color/color_accent_material</item>
    </style>

    <style name="ThemeOverlay.ActivityLauncher.Material.Dark" parent="Base.ThemeOverlay.ActivityLauncher.Material">
        <item name="android:colorPrimary">@color/color_primary_material</item>
        <item name="android:colorPrimaryDark">@color/color_primary_dark_material</item>
        <item name="android:colorAccent">@color/color_accent_material</item>
    </style>
</resources>